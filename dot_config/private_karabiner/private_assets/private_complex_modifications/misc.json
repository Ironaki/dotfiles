{
    "title": "Misc",
    "rules": [
        {
            "manipulators": [
                {
                    "description": "map ctrl+h to backspace",
                    "type": "basic",
                    "from": {
                        "key_code": "h",
                        "modifiers": {
                            "mandatory": [
                                "control"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ]
                }
            ]
        }
    ]
}
